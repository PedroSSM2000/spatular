  "resolutions": {
    "@types/react": "^17"
  },
  "private": false,
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn organize-imports-cli",
      "yarn eslint --cache --fix",
      "yarn prettier --write",
      "yarn eslint --cache"
