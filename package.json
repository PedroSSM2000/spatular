    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.1",
    "@shopify/flash-list": "1.3.1",
    "dripsy": "^3.6.0",
    "expo": "^47.0.0",
    "expo-app-loading": "~2.1.1",
    "expo-av": "~13.0.2",
    "expo-clipboard": "~4.0.1",
    "expo-font": "~11.0.1",
    "expo-linear-gradient": "~12.0.1",
    "expo-linking": "~3.2.3",
